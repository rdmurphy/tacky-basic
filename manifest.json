{
    "name": "Tacky (Basic) - a pinboard.in extension",
    "version": "0.5",
    "description": "A new pinboard.in extension that intends to combine the best features of what is currently offered.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Tacky"
    },
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": [ "key_listen.js" ]
        }
    ],
    "permissions": [ "tabs", "http://*/*", "https://*/*" ],
    "background_page": "background.html"
}
